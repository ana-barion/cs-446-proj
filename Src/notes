	 char msg[30];
	 sprintf(msg, "Cold Temperature: ");
	 print_hex_f_array(&huart2, (uint8_t*) msg, 5000, false, false, false, false, false);
	 print_hex_f_array(&huart2, (uint8_t*) ds18b20_read_temperature(&ds18b20_cold, false, &huart2), 5000, false, false, false, true, false);
	 uint8_t ch = '\n';
	 HAL_UART_Transmit(&huart2, &ch, 1, 1000);
	 sprintf(msg, "Hot Temperature: ");
	 print_hex_f_array(&huart2, (uint8_t*) msg, 5000, false, false, false, false, false);
	 print_hex_f_array(&huart2, (uint8_t*) ds18b20_read_temperature(&ds18b20_hot, false, &huart2), 5000, false, false, false, true, false);
	 HAL_UART_Transmit(&huart2, &ch, 1, 1000);
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 	 if(command != 0x00){
			switch (command){
				// SET NEW TEMPERATURE
				case 0x01:
					while (!(USART1->SR & USART_SR_RXNE));
					set_temperature(USART1->DR);
					break;

				// Read terminal water temperature
				case 0x02:
					print_hex_f_array(&huart2, (uint8_t*) ds18b20_read_temperature(&ds18b20_terminal, false, &huart2), 5000, false, false, false, false, false);
					break;

				// Read cold water temperature
				case 0x03:
					print_hex_f_array(&huart2, (uint8_t*) ds18b20_read_temperature(&ds18b20_cold, false, &huart2), 5000, false, false, false, false, false);
					break;

				//Read hot water temperature
				case 0x04:
					print_hex_f_array(&huart2, (uint8_t*) ds18b20_read_temperature(&ds18b20_hot, false, &huart2), 5000, false, false, false, false, false);
					break;

				// Turn On back pump
				case 0x05:
					turn_back_pump_on;
					break;

				// Turn On terminal pump
				case 0x06:
					turn_terminal_pump_on;
					break;

				// Turn Off back pump
				case 0x07:
					turn_back_pump_off;
					break;

				// Turn Off terminal pump
				case 0x08:
					turn_terminal_pump_off;
					break;

				// Open cold water valve and close Return cold water valve
				case 0x09:
					feed_cold_water;
					break;

				// Open Return cold water valve and close Feed water valve
				case 0x0A:
					return_cold_water;
					break;

				// Open hot water valve and close Return hot water
				case 0x0B:
					feed_hot_water;
					break;

				// Open Return hot water valve and close cold water valve
				case 0x0C:
					return_hot_water;
					break;

				// Enable timer to control the water temperature
				case 0x0D:
					break;

			    // Disable to timer, IDLE MODE.
				case 0x0E:
					break;
				default:
					break;

	 }
	command = 0x00;
		}
	 //GPIO_PinState HAL_GPIO_ReadPin(GPIO_TypeDef* GPIOx, uint16_t GPIO_Pin)
	 if(temperature_control_enable){
		 print_hex_f_array(&huart2, (uint8_t*) ds18b20_read_temperature(&ds18b20_cold, false, &huart2), 5000, false, false, false, false, false);
		 return_hot_water;
	 }